  "scripts": {
    "start": "yarn nodemon src/index.js",
    "migrate": "yarn sequelize-cli db:migrate",
    "migrate-undo": "yarn sequelize-cli db:migrate:undo:all",
    "seed": "yarn sequelize-cli db:seed:all",
    "seed-undo": "yarn sequelize-cli db:seed:undo:all",
    "create-models": "yarn sequelize-auto -h localhost -d db_movie -u root -x 1234 -p 3306 --dialect mysql -o src/models -l es6"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize": "^6.25.5",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.5.2"
  }
}
